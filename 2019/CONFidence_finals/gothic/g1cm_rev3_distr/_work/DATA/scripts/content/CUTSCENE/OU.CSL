		[]
	[]
	[% zCCSBlock 0 3607]
		blockName=string:DIA_Mud_Nerve_11_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3608]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3609]
				subType=enum:0
				text=string:Some people think I'm a complete lunatic. But somebody like you would never hang out with a lunatic. People would talk about that. So they'll finally understand that they were wrong about me.
				name=string:DIA_MUD_NERVE_11_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 3610]
		blockName=string:DIA_Mud_Nerve_12_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3611]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3612]
				subType=enum:0
				text=string:You are my best friend here. The others only want to get rid of me all the time. I'm really grateful to you.
