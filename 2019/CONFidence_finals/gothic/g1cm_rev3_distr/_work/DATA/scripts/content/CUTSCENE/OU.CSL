			[]
		[]
	[]
	[% zCCSBlock 0 3631]
		blockName=string:DIA_Mud_Nerve_18_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3632]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3633]
				subType=enum:0
				text=string:Now that you are with me, we could show a few people where to go. These bastards have been harassing me for years. When I see one of them now, I'm gonna him a few things.
