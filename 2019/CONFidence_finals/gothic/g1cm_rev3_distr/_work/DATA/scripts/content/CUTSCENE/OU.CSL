			[]
		[]
	[]
	[% zCCSBlock 0 3634]
		blockName=string:DIA_Mud_Nerve_19_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3635]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3636]
				subType=enum:0
				text=string:The two of us are a dream team. We could take over the Old Camp. The Ore Barons wouldn't be a problem for us since we are a team. I'll think of a plan.
				name=string:DIA_MUD_NERVE_19_07_00.WAV
