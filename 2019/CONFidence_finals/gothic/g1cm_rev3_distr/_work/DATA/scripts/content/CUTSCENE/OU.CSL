		subBlock0=float:0
		[% zCCSAtomicBlock 0 3605]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3606]
				subType=enum:0
				text=string:You know what? I'll do you a favor. I'll put in a good word for you, then perhaps you'll be admitted to the Camp. Then we could meet every day.
				name=string:DIA_MUD_NERVE_10_07_00.WAV
