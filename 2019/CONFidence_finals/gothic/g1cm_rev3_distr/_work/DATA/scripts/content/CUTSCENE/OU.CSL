		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3617]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3618]
				subType=enum:0
				text=string:You're like a big brother to me. I never had a big brother. Or a small one. My parents didn't want me. But I'm sure it was difficult for them.
				name=string:DIA_MUD_NERVE_14_07_00.WAV
