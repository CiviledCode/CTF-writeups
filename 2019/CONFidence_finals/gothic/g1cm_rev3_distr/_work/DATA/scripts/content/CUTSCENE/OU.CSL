	[]
	[% zCCSBlock 0 3655]
		blockName=string:DIA_Mud_Nerve_7_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3656]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3657]
				subType=enum:0
				text=string:Why did they put you in here anyway? Do you like animals as well? They're not into that one bit. That's why they locked me in here.
				name=string:DIA_MUD_NERVE_7_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 3658]
		blockName=string:DIA_Mud_Nerve_8_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3659]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3660]
				subType=enum:0
				text=string:There are some people who constantly try to beat me up. Horrible guys from the New Camp. Real cut-throats. The next time I'll tell them that, they will have to deal with you in future. Then they'll be dead scared.
				name=string:DIA_MUD_NERVE_8_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 3661]
		blockName=string:DIA_Mud_Nerve_9_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3662]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3663]
				subType=enum:0
				text=string:Those bastards from the New Camp will be amazed when they get to know that I have a protector. They'll piss themselves. That'll be fun. Normally they finish people off quickly, but now they'll think twice.
				name=string:DIA_MUD_NERVE_9_07_00.WAV
			[]
		[]
