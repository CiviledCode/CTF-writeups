			[]
		[]
	[]
	[% zCCSBlock 0 3649]
		blockName=string:DIA_Mud_Nerve_5_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3650]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3651]
