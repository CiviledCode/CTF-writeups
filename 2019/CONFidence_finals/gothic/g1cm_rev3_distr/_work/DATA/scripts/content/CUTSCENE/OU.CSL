			[]
		[]
	[]
	[% zCCSBlock 0 3646]
		blockName=string:DIA_Mud_Nerve_4_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 3647]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 3648]
				subType=enum:0
				text=string:Is it possible that we're walking in circles? I mean, I don't know where you want to go...
